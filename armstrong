//Write a program to print out all Armstrong numbers between 100 to 500.



#include<stdio.h>

void main()
{
int number,rem,sum,temp;



for(num=100; number<=500; number++)
	{
	temp=number;
	sum=0;
	while(temp!=0)
		
		
		
	{
		rem=temp%10;
		temp=temp/10;
		sum+=(rem*rem*rem);
	}
	if(num==sum)
		printf("%d\n", number);
	}

}
